+//*****************************************************************************
+// GSA - GPS DOP and Active Satellites
+// $--GSA,a,a,xx,xx,xx,xx,xx,xx,xx,xx,xx,xx,xx,xx,x.x,x.x,x.x*hh
+bool NMEA0183ParseGSA_nc(const tNMEA0183Msg &NMEA0183Msg, tGSA &gsa) {
+    bool result = (NMEA0183Msg.FieldCount() >= 17);
+    
+    if (result) {
+        // Mode: M=Manual, A=Automatic
+        gsa.mode = NMEA0183Msg.Field(0)[0];
+        
+        // Fix type: 1=no fix, 2=2D, 3=3D
+        gsa.fixType = atoi(NMEA0183Msg.Field(1));
+        
+        // PRNs of satellites used (fields 2-13)
+        for (int i = 0; i < 12; i++) {
+            const char *field = NMEA0183Msg.Field(i + 2);
+            if (field[0] != 0) {
+                gsa.satellitePRN[i] = atoi(field);
+            } else {
+                gsa.satellitePRN[i] = 0;  // Empty field
+            }
+        }
+        
+        // PDOP, HDOP, VDOP
+        gsa.PDOP = NMEA0183GetDouble(NMEA0183Msg.Field(14));
+        gsa.HDOP = NMEA0183GetDouble(NMEA0183Msg.Field(15));
+        gsa.VDOP = NMEA0183GetDouble(NMEA0183Msg.Field(16));
+    }
+    
+    return result;
+}
+
+//*****************************************************************************
+bool NMEA0183SetGSA(tNMEA0183Msg &NMEA0183Msg, const tGSA &gsa, const char *Src) {
+    if (!NMEA0183Msg.Init("GSA", Src)) return false;
+    
+    // Mode
+    char modeStr[2];
+    modeStr[0] = (gsa.mode == 'M' || gsa.mode == 'A') ? gsa.mode : 'A';
+    modeStr[1] = '\0';
+    if (!NMEA0183Msg.AddStrField(modeStr)) return false;
+    
+    // Fix type
+    if (!NMEA0183Msg.AddUInt32Field(gsa.fixType)) return false;
+    
+    // Satellite PRNs (12 fields)
+    for (int i = 0; i < 12; i++) {
+        if (gsa.satellitePRN[i] > 0) {
+            if (!NMEA0183Msg.AddUInt32Field(gsa.satellitePRN[i])) return false;
+        } else {
+            if (!NMEA0183Msg.AddEmptyField()) return false;
+        }
+    }
+    
+    // PDOP
+    if (!NMEA0183Msg.AddDoubleField(gsa.PDOP)) return false;
+    
+    // HDOP
+    if (!NMEA0183Msg.AddDoubleField(gsa.HDOP)) return false;
+    
+    // VDOP
+    if (!NMEA0183Msg.AddDoubleField(gsa.VDOP)) return false;
+    
+    return true;
+}
 
 	  
 	  
