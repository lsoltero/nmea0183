+//*****************************************************************************
+// GSA - GPS DOP and Active Satellites
+struct tGSA {
+    char mode;              // M=Manual, A=Automatic
+    uint8_t fixType;        // 1=no fix, 2=2D, 3=3D
+    uint32_t satellitePRN[12];  // PRNs of satellites used for fix (0 = not used)
+    double PDOP;            // Position Dilution of Precision
+    double HDOP;            // Horizontal Dilution of Precision  
+    double VDOP;            // Vertical Dilution of Precision
+};
+
+//*****************************************************************************
+// GSA - GPS DOP and Active Satellites
+// $--GSA,a,a,xx,xx,xx,xx,xx,xx,xx,xx,xx,xx,xx,xx,x.x,x.x,x.x*hh
+bool NMEA0183ParseGSA_nc(const tNMEA0183Msg &NMEA0183Msg, tGSA &gsa);
+
+inline bool NMEA0183ParseGSA(const tNMEA0183Msg &NMEA0183Msg, tGSA &gsa) {
+    return (NMEA0183Msg.IsMessageCode("GSA") ?
+            NMEA0183ParseGSA_nc(NMEA0183Msg, gsa) : false);
+}
+
+bool NMEA0183SetGSA(tNMEA0183Msg &NMEA0183Msg, const tGSA &gsa, const char *Src="GP");
+
 #endif
